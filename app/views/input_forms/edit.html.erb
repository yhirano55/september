<%= render(Primer::Beta::Heading.new(tag: :h1)) { "Editing input form" } %>

<%= primer_form_with(model: @input_form) do |f| %>
  <%= render InputFormForm.new(f) %>
<% end %>

<%= render(Primer::Beta::Button.new(tag: :a, href: input_form_path(@input_form), block: :true, mt: 2)) { "Show this input form" } %>
<%= render(Primer::Beta::Button.new(tag: :a, href: input_forms_path, block: :true, mt: 2)) { "Back to input forms" } %>
